{
  "name": "@rbxts/easy-path",
  "version": "1.0.3",
  "description": "This package will help you build paths with smooth turns",
  "main": "out/init.lua",
  "scripts": {
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "test:watch": "rbxtsc -w --type=game -p test -i test/include",
    "serve": "rojo serve test/default.project.json"
  },
  "keywords": [
    "EasyPath",
    "Path",
    "Tower defense"
  ],
  "repository": "https://github.com/Tesmi-Develop/easy-path",
  "author": "tesmi",
  "license": "MIT",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "!**/*.tsbuildinfo",
    "!**/*.spec.lua"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^2.2.0-types.0",
    "@rbxts/types": "^1.0.744",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "prettier": "^3.2.4",
    "roblox-ts": "^2.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@rbxts/bezier": "^0.2.0",
    "@rbxts/services": "^1.5.1"
  }
}
